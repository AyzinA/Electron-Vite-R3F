{
  "name": "electron-vite-r3f",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "build": {
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "dist/**",
      "dist-electron/**"
    ],
    "asar": true,
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": false
    }
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build && vite build --config vite.config.ts",
    "dist:win": "npm run build && electron-builder --win"
  },
  "dependencies": {
    "@react-three/drei": "^9.105.0",
    "@react-three/fiber": "^8.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.159.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.3.0",
    "electron-builder": "^26.0.12",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-electron": "^0.28.0"
  }
}